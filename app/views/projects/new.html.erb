<%= link_to 'Back to Launch Center', projects_path %>

<%= form_for @project do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :mission_statement %>
  <%= f.text_field :mission_statement %>

  <%= f.submit :launch %>

<% end %>

<%= button_to 'Donate', new_charge_path(current_user), method: :get %>