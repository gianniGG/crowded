<% if user_signed_in? %>
  <li>
    <%= link_to 'Launch Center', projects_path %>
  </li>
  <li>
  <%= link_to('Logout', destroy_user_session_path, :method => :delete) %>
  </li>
  <li>
  <%= link_to 'Profile', user_path(current_user) %>
  </li>
<% else %>
  <li>
  <%= link_to('Login as User', new_user_session_path)  %>
  </li>
<% end %>

<% if company_signed_in? %>
  <li>
    <%= link_to 'Launch Center', projects_path %>
  </li>
  <li>
  <%= link_to('Logout', destroy_company_session_path, :method => :delete) %>
  </li>
  <li>
  <%= link_to 'Profile', company_path(current_company) %>
  </li>
<% else %>
  <li>
  <%= link_to('Login as Company', new_company_session_path)  %>
  </li>
<% end %>
